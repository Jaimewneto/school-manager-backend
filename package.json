{
  "name": "school-manager-backend",
  "version": "1.11.8",
  "description": "School Manager Backend",
  "main": "dist/index.js",
  "repository": "https://github.com/Jaimewneto/school-manager-backend.git",
  "author": "Jaime Weing√§rtner Neto",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "pnpm exec tsc --noEmit --pretty && eslint . --ext .ts",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/app.ts",
    "dev:debug": "ts-node-dev -r tsconfig-paths/register --respawn --inspect=4321 --transpile-only --ignore-watch node_modules --no-notify src/app.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "build:tsx": "tsc && tsc-alias",
    "token:generate-system-owner": "tsx src/scripts/generate-system-owner-token.ts",
    "token:generate-company": "tsx src/scripts/generate-company-token.ts",
    "token:generate-pdv": "tsx src/scripts/generate-pdv-token.ts",
    "s3:test": "tsx src/scripts/test-s3.ts",
    "db:make-migration": "npx knex migrate:make -x ts --knexfile knexfile.config.ts --env main",
    "db:make-seed": "npx knex seed:make -x ts --knexfile knexfile.config.ts --env main",
    "db:rollback": "npx knex migrate:rollback ts --knexfile knexfile.config.ts --env main",
    "db:migrate": "npx knex migrate:latest ts --knexfile knexfile.config.ts --env main"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/formbody": "^8.0.2",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "fastify": "^5.4.0",
    "fastify-plugin": "^5.1.0",
    "fastify-type-provider-zod": "^4.0.2",
    "getenv": "^1.0.0",
    "jose": "^6.1.0",
    "knex": "^3.1.0",
    "luxon": "^3.6.1",
    "pg": "^8.13.1",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/eslint": "~8.56.10",
    "@types/eslint-config-prettier": "~6.11.3",
    "@types/getenv": "^1.0.3",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.13.0",
    "@types/pg": "^8.11.11",
    "@types/socket.io": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "prettier": "^3.4.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  }
}
